public class App 
{
	public static void main(String[] args) 
	{
		int[] x=new int[3];
		System.out.println("Size of x:"+x.length);
		x[0]=10;
		x[1]=20;
		x[2]=90;
		//code to change size of array
		int[] y=new int[x.length+x.length/2];
		for(int i=0;i<x.length;i++)
		{
			y[i]=x[i];
		}
		x=y;
		x[3]=80;
		System.out.println("Size of x:"+x.length);
	}
}
