import java.util.*;

public class App4 
{
	public static void main(String[] args) 
	{
		HashMap<String,String> map=new HashMap<String, String>();
		map.put("amit","9887777664");
		map.put("manoj","1234567890");
		map.put("imran","8810436567");
		map.put("kuldeep","4433556677");
		map.put("arun","3377448899");
		map.remove("manoj");
		System.out.println(map);
		
	}
}
